<h1>Create a New Cocktail</h1>
<div class="container">
  <%= simple_form_for @cocktail do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit, class: "btn-primary" %>
</div>

<%= link_to "back to Cocktail List" , cocktails_path  %>
<% end %>
